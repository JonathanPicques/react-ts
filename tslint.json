{
    "rules": {
        "ban": false,
        "radix": true,
        "align": [true, "parameters", "arguments", "statements"],
        "forin": true,
        "curly": true,
        "no-arg": true,
        "no-any": true,
        "indent": [true, "spaces"],
        "no-eval": true,
        "eofline": false,
        "typedef": [true, "parameter", "property-declaration"],
        "no-empty": true,
        "one-line": [true, "check-catch", "check-else", "check-open-brace", "check-whitespace"],
        "quotemark": [true, "single", "jsx-double"],
        "semicolon": [true, "always", "strict-bound-class-methods"],
        "whitespace": [true, "check-branch", "check-decl", "check-operator", "check-separator", "check-type", "check-typecast"],
        "no-console": [false, "log", "error", "debug", "info", "time", "timeEnd", "trace"],
        "class-name": true,
        "no-bitwise": true,
        "no-debugger": true,
        "jsdoc-format": true,
        "no-construct": true,
        "jsx-no-lambda": false,
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case"],
        "triple-equals": [true, "allow-null-check"],
        "trailing-comma": [false],
        "interface-name": [true, "never-prefix"],
        "comment-format": [true, "check-space"],
        "switch-default": true,
        "label-position": true,
        "member-ordering": [true, "public-before-private", "static-before-instance", "variables-before-functions"],
        "max-line-length": [true, 160],
        "no-string-literal": true,
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "jsx-no-multiline-js": false,
        "no-unused-expression": true,
        "no-shadowed-variable": true,
        "no-use-before-declare": true,
        "no-duplicate-variable": true,
        "no-trailing-whitespace": false,
        "no-consecutive-blank-lines": true,
        "no-switch-case-fall-through": true
    }
}
